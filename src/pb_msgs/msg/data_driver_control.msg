# Структура топика с моими данными получаемыми с нижнего уровня Body

uint32  id                      # id сообщения 
uint32 status_wifi              # Статус запущен ли вайфай или выключен
uint32 connect_flag             # Флаг связи с пультом ручного управления
uint32 startStop                # Стоим или двигаемся
float32 radius                  # Радиус по которому нужно двигаться
float32 speed                   # Скорость с которой нужно двигаться
float32 angle_camera            # Угол камеры для шагового мотора камеры
uint32 led                      # Номер программы для светодиодов мигания
uint32 servo                    # Номер программы для сервомоторов движения рук
int32 posServoL                 # Позиция левого сервомотра
int32 posServoR                 # Позиция правого сервомотра




