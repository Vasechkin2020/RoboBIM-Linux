{
  "configurations": [
      {
          "name": "ROS",
          "browse": {
              "databaseFilename": "",
              "limitSymbolsToIncludedHeaders": true
          },
          "includePath": [
              "${workspaceFolder}/devel/include",
         //     "${workspaceFolder}/src/data/include",
         //     "${workspaceFolder}/src/data/include/data",
              "${workspaceFolder}/src/data/src",
              "${workspaceFolder}/src/",
              "/opt/ros/melodic/include/**",
              "/usr/include/**",
              "${workspaceFolder}/**"
          ],
          "intelliSenseMode": "gcc-arm",

          "compilerPath": "/usr/bin/g++",
          "cStandard": "c11",
          "cppStandard": "c++17",
          // "compilerPath": "/usr/bin/gcc",
          // "cStandard": "gnu17",
          // "cppStandard": "gnu++14"
          "compileCommands": "${workspaceFolder}/build/compile_commands.json",
          "configurationProvider": "ms-vscode.cmake-tools"
      }
  ],
  "version": 4
}